[
	{ "keys": [":", ";"], "type": "function", "command": "enter_command_line_mode" },
	{ "keys": ["TAB", "i"], "type": "function", "command": "focus_next" },
	{ "keys": ["SHIFT_TAB", "n"], "type": "function", "command": "focus_previous" },
	{ "keys": ["UP", "u"], "type": "function", "command": "move_up" },
	{ "keys": ["DOWN", "e"], "type": "function", "command": "move_down" },
	{ "keys": ["PAGE_UP", "U"], "type": "function", "command": "page_up" },
	{ "keys": ["PAGE_DOWN", "E"], "type": "function", "command": "page_down" },
	{ "keys": ["ENTER", "o"], "type": "function", "command": "activate" },
	{ "keys": ["DELETE", "dd"], "type": "function", "command": "delete" },
	{ "keys": [" "], "command": "todo toggle $n" },
	{ "keys": ["tt"], "type": "function", "command": "toggle_console" },
	{ "keys": ["tm"], "type": "function", "command": "toggle_metadata" },

	{ "keys": ["/"], "type": "prompt", "command": "search \"\"", "cursorPosition": -2 },
	{ "keys": ["mm"], "type": "prompt", "command": "mknote \"\"", "cursorPosition": -2 },
	{ "keys": ["mt"], "type": "prompt", "command": "mktodo \"\"", "cursorPosition": -2 },
	{ "keys": ["mb"], "type": "prompt", "command": "mkbook \"\"", "cursorPosition": -2 },
	{ "keys": ["yy"], "type": "prompt", "command": "cp $n \"\"", "cursorPosition": -2 },
	{ "keys": ["dn"], "type": "prompt", "command": "mv $n \"\"", "cursorPosition": -2 },
	{ "keys": ["Q"], "type": "prompt", "command": "exit" },
	{ "keys": ["S"], "type": "prompt", "command": "sync" }
